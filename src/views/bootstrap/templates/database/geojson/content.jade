.container-fluid
  .row
    .col-xs-12
      .page-header
        h1 Datenbank
      h2 Geojson Werte
      table.table.table-hover.table-condensed
        thead
            tr
              th id
              th district
              th district_name
              th number
              th nuts3
              th nuts2
              th nuts1
              th nuts0
              th region_name
          tbody
            tr(ng-repeat="nut in nuts")
              td {{data.id}}
              td {{data.district}}
              td {{data.district_name}}
              td {{data.number}}
              td {{data.nuts3}}
              td {{data.nuts2}}
              td {{data.nuts1}}
              td {{data.nuts0}}
              td {{data.region_name}}
