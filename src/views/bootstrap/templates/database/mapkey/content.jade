.container-fluid
  .row
    .col-xs-12
      .page-header
        h1 Datenbank
      h2 Highmap Mapkey's
      span(ng-repeat="(level, levelmapkeys) in mapkeys")
        h3 {{level}}
        table.table.table-hover.table-condensed
          thead
              tr
                th mapkey
            tbody
              tr(ng-repeat="mapkey in levelmapkeys")
                td
                  a(ng-href="/#/database/geojson-mapkey/{{mapkey}}") {{mapkey}}
