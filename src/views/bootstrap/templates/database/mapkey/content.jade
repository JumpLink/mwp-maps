.container-fluid
  .row
    .col-xs-12
      .page-header
        h1 Datenbank
      h2 Highmap Mapkey's
      table.table.table-hover.table-condensed
        thead
          tr
            th mapkey
            th adminlevel
            th nutslevel
        tbody
          tr(ng-repeat="mapkey in mapkeys")
            td
              a(ng-href="/#/database/geojson-mapkey/{{mapkey.mapkey}}") {{mapkey.mapkey}}
            td {{mapkey.adminlevel}}
            td {{mapkey.nutslevel}}
