.container-fluid
  .row
    .col-xs-12
      .page-header
        h1 Datenbank
      h2 Features
      table.table.table-hover.table-condensed
        thead
            tr
              th hasc
              th hc-key
              th adminlevel
              th nutslevel
              th woe-id
              th name
              th geometry
          tbody
            tr(ng-repeat="feature in features")
              td {{feature.properties.hasc}}
              td {{feature.properties['hc-key']}}
              td {{feature.properties.adminlevel}}
              td {{feature.properties.nutslevel}}
              td {{feature.properties['woe-id']}}
              td {{feature.properties.name}}
